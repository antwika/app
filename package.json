    "ts-jest": "^27.1.4"
  },
  "dependencies": {
    "@antwika/common": "0.0.2",
    "componentsjs": "^5.0.1",
    "componentsjs-generator": "^3.0.2",
    "typescript": "^4.6.3"
  },
